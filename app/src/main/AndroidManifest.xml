<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ida.myapplication">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/logga"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logga"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >

        <activity android:name="com.example.ida.dressyr_appen.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.example.ida.dressyr_appen.ExercisesActivity"
            android:parentActivityName="com.example.ida.dressyr_appen.MainActivity"/>

        <activity android:name="com.example.ida.dressyr_appen.ProgramActivity"
            android:parentActivityName="com.example.ida.dressyr_appen.MainActivity"/>

        <activity android:name="com.example.ida.dressyr_appen.ProgramWebActivity"
            android:parentActivityName="com.example.ida.dressyr_appen.ProgramActivity"
            />

        <activity android:name="com.example.ida.dressyr_appen.ContactActivity"/>

    </application>
</manifest>